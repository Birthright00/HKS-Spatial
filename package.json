{
  "name": "hks-spatial-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Root for HKS Spatial Design Studio",
  "scripts": {
    "install-all": "npm install && npm run install-backend && npm run install-frontend",
    "install-backend": "npm install --prefix backend",
    "install-frontend": "npm install --prefix Spatial-Design-Studio-Frontend/frontend",
    "dev": "concurrently \"npm:backend\" \"npm:frontend\"",
    "backend": "npm run dev --prefix backend",
    "frontend": "npm run dev --prefix Spatial-Design-Studio-Frontend/frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
